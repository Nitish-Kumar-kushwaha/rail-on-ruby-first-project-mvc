<div class="container">
    <h1>All Jobs</h1>
    <ul>
        <% @jobs.each do |job| %>
        <li>
            <%= job.id%>  <%= link_to job.title, job_path(job)%>
        </li>
        <% end %>
    </ul>

    <%= link_to "Create New Job", new_job_path, class: "btn btn-md btn-success"%>

</div>
